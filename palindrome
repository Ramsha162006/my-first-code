#include<iostream>
using namespace std;
bool isPalindrome(string str){
  int start = 0;
  int end = str.length() - 1;
  while(start<end){
  if(str[start]!=str[end])
  return false;
  start ++;
  end --;
  }
  return true;
  }
  int main()
  {
  string input;
  cout<<"enter a string:";
  cin>>input;
  if (isPalindrome(input))
  cout<<"the string is palindrome."<<endl;
  else
  cout<<"the string is not a palindrome."<<endl;
  }
